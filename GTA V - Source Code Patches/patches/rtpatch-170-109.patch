new file mode 100644
index 000000000..a0efd2d50
--- /dev/null
+++ b/rage/base/src/system/rtpatchversion.cpp
@@ -0,0 +1,66 @@
+#include "rtpatchversion.h"
+
+namespace rage
+{
+	namespace rtPatchVersion {
+		// Update this value when updating a published patch version
+		char m_sRTPatchBinaryVersionId[32] = "1.0.9(3521)";
+		char m_sRTPatchGameVersionId[64] = "0";
+		char m_sRTPatchOnlineVersionId[64] = "1.0.0";
+		const int m_iVersionUpperLimit = 3521;
+		const int m_iVersionLowerLimit = 3407;
+		bool m_bHasShownWarning = false;
+
+		const char* GetRTPatchVersionBinaryVersionNumber() {
+			return m_sRTPatchBinaryVersionId;
+		}
+
+		const char* GetRTPatchVersionGameVersionNumber() {
+			return m_sRTPatchGameVersionId;
+		}
+
+		const char* GetRTPatchVersionOnlineVersionNumber() {
+			return m_sRTPatchOnlineVersionId;
+		}
+
+		void SetRTPatchVersionGameVersionNumber(const char* version) {
+			strncpy(m_sRTPatchGameVersionId, version, sizeof(m_sRTPatchGameVersionId) - 1);
+			m_sRTPatchGameVersionId[sizeof(m_sRTPatchGameVersionId) - 1] = '\0';
+		}
+
+		void SetRTPatchVersionOnlineVersionNumber(const char* version) {
+			strncpy(m_sRTPatchOnlineVersionId, version, sizeof(m_sRTPatchOnlineVersionId) - 1);
+			m_sRTPatchOnlineVersionId[sizeof(m_sRTPatchOnlineVersionId) - 1] = '\0';
+		}
+
+		const int* GetRTPatchVersionVersionUpperLimit() {
+			return &m_iVersionUpperLimit;
+		}
+
+		const int* GetRTPatchVersionVersionLowerLimit() {
+			return &m_iVersionLowerLimit;
+		}
+
+		void SetHasShownWarning(bool value) {
+			m_bHasShownWarning = value;
+		}
+
+		bool GetHasShownWarning() {
+			return m_bHasShownWarning;
+		}
+
+		bool CheckVersionCompatibility() {
+			char* versionStr = m_sRTPatchGameVersionId;
+			if (versionStr[0] == '\0')
+				return false;
+
+			int versionNumber = 0;
+			while (*versionStr >= '0' && *versionStr <= '9') {
+				versionNumber = versionNumber * 10 + (*versionStr - '0');
+				versionStr++;
+			}
+
+			return versionNumber >= m_iVersionLowerLimit && versionNumber <= m_iVersionUpperLimit;
+		}
+	}
+}
