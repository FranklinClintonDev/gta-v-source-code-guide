+int CommandGetVehicleTrailerParent(int iVehicleIndex)
+{
+	const CVehicle* pVehicle = CTheScripts::GetEntityToModifyFromGUID<CVehicle>(iVehicleIndex);
+	if (pVehicle)
+	{
+		const CVehicle* pParentVehicle = pVehicle->GetAttachParentVehicle();
+		if (pParentVehicle)
+		{
+			return CTheScripts::GetGUIDFromEntity(*pVehicle->GetAttachParentVehicle());
+		}
+	}
+	return -1;
+}
